{
  "name": "hl7-capture",
  "version": "1.0.15",
  "description": "Network traffic capture desktop application built with Electron and React",
  "main": ".vite/build/main.js",
  "author": "Glenn",
  "license": "MIT",
  "scripts": {
    "start": "electron-forge start",
    "dev": "electron-forge start",
    "dev:renderer": "vite",
    "package": "electron-forge package",
    "premake": "node scripts/generate-icons.js",
    "make": "node scripts/make-and-tag.js",
    "postmake": "node scripts/update-exe-icon.js",
    "make:all": "npm run premake && npm run make && npm run postmake",
    "fix-update-icons": "node scripts/update-exe-icon.js",
    "postinstall": "electron-rebuild && node scripts/setup-npcap.js",
    "send-hl7": "node scripts/send-hl7.js --host 127.0.0.1 --port 6000 --message-file scripts/samples/msg1.hl7 --proto tcp --frame stx --listen --reply-message ACK --reply-frame stx",
    "mllp-sim": "node scripts/mllp-sim.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts,.tsx",
    "format": "prettier --write 'src/**/*.{ts,tsx,css}'",
    "clean": "node scripts/clean.mjs"
  },
  "dependencies": {
    "@radix-ui/react-slot": "^1.2.4",
    "@types/react-window": "^1.8.8",
    "electron-squirrel-startup": "^1.0.1",
    "pcap-parser": "^0.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-resizable-panels": "^3.0.6",
    "react-window": "^2.2.2",
    "winston": "^3.18.3",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.2.1",
    "@electron-forge/maker-deb": "^6.2.1",
    "@electron-forge/maker-rpm": "^6.2.1",
    "@electron-forge/maker-squirrel": "^6.2.1",
    "@electron-forge/maker-zip": "^6.2.1",
    "@electron-forge/plugin-vite": "^6.2.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^14.1.0",
    "@types/electron-squirrel-startup": "^1.0.2",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^8.46.3",
    "@vitejs/plugin-react": "^4.2.0",
    "autoprefixer": "^10.4.21",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "electron": "^27.0.0",
    "electron-rebuild": "^3.2.9",
    "eslint": "^8.53.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.2.0",
    "lucide-react": "^0.552.0",
    "png-to-ico": "^3.0.1",
    "postcss": "^8.5.6",
    "prettier": "^3.1.0",
    "rcedit": "^5.0.1",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^3.4.18",
    "tailwindcss-animate": "^1.0.7",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "vite": "^5.0.0"
  }
}
